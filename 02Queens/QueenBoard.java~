public class QueenBoard{
    private int[][] board;
    
    public QueenBoard(int size) {
	board = new board[size][size];
    }

    public String toString() {
	String ans = "";
	for (int r = 0; r < board.length; r++) {
	    for (int c = 0; c < board[0].length; c++) {
		ans += board[r][c] + " ";
	    }
	    ans += "\n";
	}
	return ans;
    }
    public boolean solve() {
	
    }
}
